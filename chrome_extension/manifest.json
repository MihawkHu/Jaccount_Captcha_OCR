{
    "manifest_version": 2,
    "name": "上海交大Jaccount自动填写验证码插件",
    "version": "1.1",
    "description": "Using in Jaccount login page to help input the captcha",
    "icons": {
        "16": "image/icon.png",
        "48": "image/icon.png",
        "128": "image/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "image/icon.png",
            "38": "image/icon.png"
        },
        "default_title": "Jaccount_Captcha_OCR",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://jaccount.sjtu.edu.cn/jaccount/jalogin*"],
            "js": [
                "js/theta1.js",
                "js/theta2.js",
                "js/ocr.js",
                "js/run.js"
            ]
        }
    ]
}
